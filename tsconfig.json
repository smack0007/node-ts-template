{
  "compilerOptions": {
    "declaration": true,
    "emitDeclarationOnly": true,
    "target": "ESNext",
    "allowImportingTsExtensions": true,
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  }
}
